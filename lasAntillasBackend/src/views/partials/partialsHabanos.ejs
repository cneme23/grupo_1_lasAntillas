<div
  class="categoria container-wide container-xs m-bottom m-bottom-quarter-xs m-none-xs fluid">
<div class="span8 left"> 
  <div class="m-bottom-half m-top-half">
    <h1 class="active breadcrumb-crumb">Habanos </h1>
  </div>    </div>

 
</div>

<div class="card-group container-wide span12 grid-row ">
  <% for(let i = 0; i < productoHabanos.length; i++) { %>

    <div class="card card1 cartinha ">
    <a href="/productDetailHabano/?productId=<%=productoHabanos[i].id%>"
      ><img
        src="/images/habanos/<%= productoHabanos[i].imagen%>"
        class="card-img-top"
        alt="Habano X."
        style="border: 5px solid #343a40"
    /></a>
    <div class="card-body">
      <a href="/productDetailHabano/?productId=<%=productoHabanos[i].id%>"
        ><h5 class="card-title"><%= productoHabanos[i].nombre.value%></h5></a
      >
      <p class="card-text">$<%=productoHabanos[i].precio.value%></p>
    </div>

  </div>
  <% } %>
</div>
