<div
  class="categoria container-wide container-xs m-bottom m-bottom-quarter-xs m-none-xs fluid">
<div class="span8 left"> 
  <div class="m-bottom-half m-top-half">
    <h1 class="active breadcrumb-crumb">Habanos</h1>
  </div>    </div>

  <div class="hidden-phone sort-by-container text-right">
    <label for="sort" class="d-inline">Ordenar por:</label>
    <select id="sort" class="full-width-xs">
      <option value="price-ascending">Precio: Menor a Mayor</option>
      <option value="price-descending">Precio: Mayor a Menor</option>
      <option value="alpha-ascending">A - Z</option>
      <option value="alpha-descending">Z - A</option>
      <option value="created-descending">Más Nuevo al más Viejo</option>
      <option value="created-ascending">Más Viejo al más Nuevo</option>
      <option value="best-selling" selected="">Más Vendidos</option>
    </select>
  </div>
</div>

<div class="card-group container-wide span12 grid-row ">
  <% for(let i = 0; i < productoHabanos.length; i++) { %>

    <div class="card card1 cartinha ">
    <a href="/productDetailHabano/?productId=<%=productoHabanos[i].id%>"
      ><img
        src="/images/habanos/<%= productoHabanos[i].imagen%>"
        class="card-img-top"
        alt="Habano X."
        style="border: 5px solid #343a40"
    /></a>
    <div class="card-body">
      <a href="/productDetailHabano/?productId=<%=productoHabanos[i].id%>"
        ><h5 class="card-title"><%= productoHabanos[i].nombre.value%></h5></a
      >
      <p class="card-text">$<%=productoHabanos[i].precio.value%></p>
    </div>

  </div>
  <% } %>
</div>
